{
    "name": "dev/bames",
    "description": "bames",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "docker": "docker-compose down && docker-compose --env-file .env up -d",
        "coverage": "php8.3 vendor/bin/pest --coverage --coverage-html coverage"
    },
    "authors": [
        {
            "name": "Matheus Maydana",
            "email": "mattmaydana@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "php-di/php-di": "^6.4",
        "firebase/php-jwt": "^6.3",
        "php-amqplib/php-amqplib": "^3.5",
        "phpmailer/phpmailer": "^6.8",
        "ext-pdo": "*",
        "larapack/dd": "^1.1",
        "predis/predis": "^2.2",
        "kreait/firebase-php": "^7.0",
        "google/apiclient": "^2.15.0",
        "ext-curl": "*",
        "team-reflex/discord-php": "^7.3",
        "aws/aws-sdk-php": "^3.324",
        "phpoffice/phpword": "^1.3",
        "ext-zip": "*"
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.5",
        "mockery/mockery": "^1.5"
    }
}
